<div>
<div class="jobsky-block-first" style="padding: 20px 0 20px 0;">
<div class="container">
    <div class="col-md-8">
        <div id="jobsky-carousel-generic" class="carousel slide" data-ride="carousel" style="max-width:720px;">
        <!-- 轮播（Carousel）指标：li的data-target要等于轮播导航里a的id还要等于最顶层div的id-->
        <ol class="carousel-indicators" style="width: 100px; right:10px;margin: 0 0 -10px 0;left:inherit;">
            
                @for (int i = 0; i < ViewBag.tb_Carousel.Rows.Count; ++i)
                {
                    if(i == 0){
                        <li data-target="#jobsky-carousel-generic" data-slide-to="@i" class="active"></li>
                    }
                    else{
                        <li data-target="#jobsky-carousel-generic" data-slide-to="@i" class=""></li>
                    }
                }
                
            
        </ol>
        <!-- 轮播（Carousel）项目 -->
        <div class="carousel-inner" role="listbox">
            @for (int i = 0; i < ViewBag.tb_Carousel.Rows.Count; ++i)
            {
                if (i == 0)
                {
                    if (Convert.ToInt32(ViewBag.tb_Carousel.Rows[i]["NewsID"]) == 866) { 
                    <div class="item active">
                        <img data-src="" alt="" src="~/NewsUploadFiles/@ViewBag.tb_Carousel.Rows[i]["ImgAddr"]" data-holder-rendered="true">
                        <div class="jobsky-nivo-caption"><a href="http://www.moe.gov.cn/jyb_xwfb/xw_zt/moe_357/jyzt_2015nztzl/2015_zt20/" class="" target="_blank">@ViewBag.tb_Carousel.Rows[i]["Title"]</a></div>
                    </div>
                    }
                    else
                    {
                    <div class="item active">
                        <img data-src="" alt="" src="~/NewsUploadFiles/@ViewBag.tb_Carousel.Rows[i]["ImgAddr"]" data-holder-rendered="true">
                        <div class="jobsky-nivo-caption"><a href="@Url.Action("NewsDetails", "Home", new { id = Convert.ToInt32(@ViewBag.tb_Carousel.Rows[i]["NewsID"]) })" class="" target="_self">@ViewBag.tb_Carousel.Rows[i]["Title"]</a></div>
                    </div>
                    }
                }
                else
                {
                    if (Convert.ToInt32(ViewBag.tb_Carousel.Rows[i]["NewsID"]) == 866)
                    {
                    <div class="item">
                        <img data-src="" alt="" src="~/NewsUploadFiles/@ViewBag.tb_Carousel.Rows[i]["ImgAddr"]" data-holder-rendered="true">
                        <div class="jobsky-nivo-caption"><a href="http://www.moe.gov.cn/jyb_xwfb/xw_zt/moe_357/jyzt_2015nztzl/2015_zt20/" class="" target="_blank">@ViewBag.tb_Carousel.Rows[i]["Title"]</a></div>
                    </div>
                    }
                    else
                    { 
                    <div class="item">
                        <img data-src="" alt="" src="~/NewsUploadFiles/@ViewBag.tb_Carousel.Rows[i]["ImgAddr"]" data-holder-rendered="true">
                        <div class="jobsky-nivo-caption"><a href="@Url.Action("NewsDetails", "Home", new { id = Convert.ToInt32(@ViewBag.tb_Carousel.Rows[i]["NewsID"]) })" class="" target="_self">@ViewBag.tb_Carousel.Rows[i]["Title"]</a></div>
                    </div>
                    }
                }
            }
        </div>
        <!-- 轮播（Carousel）导航 -->
        <a class="left carousel-control" href="#jobsky-carousel-generic" role="button" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#jobsky-carousel-generic" role="button" data-slide="next">
          <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
